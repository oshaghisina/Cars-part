{
  "name": "china-car-parts-admin-panel",
  "version": "1.0.0",
  "description": "Admin panel for Chinese Auto Parts Price Bot",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix",
    "format": "prettier --write src/",
